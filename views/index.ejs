<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

</head>
<body>
<% data.forEach(poke => { %>
    <div class="container">
 <div class="card">
    
        <h1> <%= poke.id %></h1>
